# grade_calculator

inputScore = float(input("Enter your score (0-100): "))
gradeA = 90
gradeB = 80
gradeC = 70
gradeD = 60

if inputScore <= 0 or inputScore >= 100:
    print("Invalid score!")
elif inputScore >= gradeA:
    print("Grade: A - Excellent!")
elif inputScore >= gradeB:
    print("Grade: B - Very Good!")
elif inputScore >= gradeC:
    print("Grade: C - Good!")
elif inputScore >= gradeD:
    print("Grade: D - Needs Improvement.")
else:
    print("Grade: F - Failed.")
